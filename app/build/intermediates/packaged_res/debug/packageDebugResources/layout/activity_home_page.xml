            android:layout_marginStart="24dp"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/redhatdisplay"
            android:text="@string/dateText"
            android:textColor="@color/black"
            android:textSize="28sp"
            android:textStyle="bold" />
